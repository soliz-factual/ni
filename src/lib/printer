ni::module->new(name         => '/lib/printer',
                dependencies => ['ni.module:/lib/serializable',
                                 'ni.module:/lib/quote'],
                code         => <<'_');
package ni::printer;
use overload qw/<< print/;
sub new {
  my $class = shift;
  bless {fh => shift}, $class;
}
sub print {shift->{fh}->print(@_, ";\n")}
ni::context('ni.module:/lib/printer')->serialize(ni::printer->new(\*STDOUT));
_
